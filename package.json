{
  "name": "angular2-test",
  "version": "1.0.0",
  "description": "Starter kit project with angular2 and webpack",
  "main": "",
  "scripts": {
    "build": "webpack --progress --profile --colors --display-error-details --display-cached",
    "watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "clean": "rm -rf dist",
    "lint": "tslint -c tslint.json \"client/**/*.ts\" || true",
    "lite": "lite-server --baseDir=dist --port=5000",
    "serve1": " npm run watch & npm run lite",
    "serve2": "webpack-dev-server --inline --progress --profile --colors --display-error-details --display-cached --port 3000",
    "serve3": "webpack-dev-server --hot --inline --progress --colors --quiet --port=5000",
    "serve4": "npm run watch & browser-sync start --server=dist --files=\"dist/*.*\" --port 5000",
    "serve5": "webpack-dev-server --hot"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.0",
    "bootstrap": "^3.3.6",
    "es6-shim": "^0.33.3",
    "parse5": "^1.3.2",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.0",
    "zone.js": "^0.5.10"
  },
  "devDependencies": {
    "browser-sync": "^2.10.1",
    "copy-webpack-plugin": "^0.3.3",
    "es6-shim": "^0.34.0",
    "file-loader": "^0.8.5",
    "gulp-mux": "^0.2.24",
    "json-loader": "^0.5.4",
    "lite-server": "^1.3.1",
    "raw-loader": "^0.5.1",
    "ts-loader": "^0.7.2",
    "tslint": "^3.2.1",
    "tslint-eslint-rules": "^1.0.1",
    "tslint-loader": "^2.1.0",
    "typescript": "^1.7.5",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "yargs": "^3.31.0"
  },
  "keywords": [],
  "author": "",
  "license": "MIT"
}
