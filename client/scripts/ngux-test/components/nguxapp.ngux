<ScrollView ng:Selector="NGUXApp" [Background]="backgroundColor">
    <StackPanel>
        <Panel Height="220" Margin="10"  >
            <ng:NGUXChild *ngIf="showPanel" />
        </Panel>
        <Rectangle (Clicked)="togglePanel()" Height="60" Fill="Purple" Margin="10"></Rectangle>
        <ng:NGUXChild *ngFor="#f of foo" />
        <Rectangle *ngFor="#f of foo" Fill="Red" Height="30" Margin="10" />
        <ng:NGUXChild />
        <Rectangle *ngFor="#f of foo" Fill="Green" Height="30" Margin="10" />
        <ng:NGUXChild />
    </StackPanel>
</ScrollView>
