<DockPanel>
    <Rectangle Dock="Top" [Height]="height" [Background]="background" (click)="onHeaderClick()"></Rectangle>
    <!--  <TextInput (topchange)="name = $event.text" height="40" fontSize="30" placeholder="name"></TextInput>
      (click)="onSelect(hero)"-->
    <ScrollView>
        <StackPanel>
            <!-- <TextInput #myname (keyup)></TextInput>
        	<Text Value="{{myname.value}}" ></Text> -->
            <Text Value="Hello, {{title}}. I have been clicked {{clickCount}} times."></Text>
            <MyRectangle [MyWidth]="mywidth"></MyRectangle>
            <Rectangle *ngFor="#hero of heroes" Background="Yellow" Height="40">
                <Text Value="{{hero.name}}"></Text>
            </Rectangle>
        </StackPanel>
    </ScrollView>
</DockPanel>
